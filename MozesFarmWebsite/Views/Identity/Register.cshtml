@model MozesFarmWebsite.Models.LoggersModels.User
@{
	ViewBag.Title = "Register";
}

<div id="Register">
	<div class="container">
		<div id="login-row" class="row align-items-center">
			<div id="login-column" class="col-md-6">
				<div id="login-box" class="col-md-12">
					<form id="login-form" class="form" action="/Identity/Register" method="post" value="ViewBag.ReturnUrl">
						<h3 class="text-info">Rgister</h3>
						<div>
							@Html.EditorForModel()
						</div>
						<input type="hidden" name="returnUrl" value="@ViewBag.returnUrl" />
						<div class="form-group mt-3">
							<input type="submit" name="submit" class="btn btn-info btn-md" value="Register">
						</div>
					</form>
					<p>@ViewBag.Message</p>
				</div>
			</div>
		</div>
	</div>
</div>

